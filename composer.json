    "nikic/php-parser": "^5.0",
    "doctrine/dbal": "^3.8|^4.0"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0|^10.0",
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "MunguiaEr\\LaravelCleanGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MunguiaEr\\LaravelCleanGenerator\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MunguiaEr\\LaravelCleanGenerator\\CleanGeneratorServiceProvider"
