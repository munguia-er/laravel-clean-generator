{
  "name": "munguia-er/laravel-clean-generator",
  "description": "Generates Clean Architecture structure for Laravel â€” DTOs, Repositories, Services, Controllers with Web & API stacks",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "clean-architecture",
    "code-generator",
    "artisan",
    "scaffold",
    "ddd",
    "repository-pattern",
    "dto",
    "api",
    "web"
  ],
  "homepage": "https://github.com/munguia-er/laravel-clean-generator",
  "support": {
    "issues": "https://github.com/munguia-er/laravel-clean-generator/issues",
    "source": "https://github.com/munguia-er/laravel-clean-generator"
  },
  "authors": [
    {
      "name": "munguia-er",
      "homepage": "https://github.com/munguia-er"
    }
  ],
  "require": {
